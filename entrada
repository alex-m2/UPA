principal

inteiro num;
inteiro fatorial = 1;

texto ==FATORIAL==;
imprima_msg( ==FATORIAL== );

texto Numero:;
imprima_msg ( Numero );
leia (num);

para ( inteiro aux = 1; aux <= num; aux) 
fatorial = fatorial * aux; 

texto Fatorial:;
imprima_msg (Fatorial);
imprima_int ( fatorial );

fimprincipal
